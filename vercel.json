{
  "name": "md-rehman.dev",
  "version": 2,
  "scope": "my-org",
  "framework": "nextjs",
  "builds": [
    {
      "src": "apps/tv-set/next.config.js",
      "use": "@vercel/next",
      "config": { "outputDirectory": "apps/tv-set/out" }
    },
    {
      "src": "apps/p5-playground/next.config.js",
      "use": "@vercel/next",
      "config": { "outputDirectory": "apps/p5-playground/out" }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/apps/tv-set"
    },
    {
      "src": "/",
      "dest": "/apps/p5-playground"
    }
  ]
}
