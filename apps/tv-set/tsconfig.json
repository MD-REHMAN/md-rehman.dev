{
  "extends": "@repo/typescript-config/nextjs.json",
  "baseUrl": ".",
  "paths": {
    "p5-playground/*": ["src/types/*"]
  },
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "next.config.mjs",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
